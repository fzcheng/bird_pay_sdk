<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="lgsdk_custom_dialog_theme" parent="@android:style/Theme.Dialog">
	    <item name="android:windowBackground">@android:color/transparent</item>
	    <item name="android:windowNoTitle">true</item>
	</style>
	
    <style name="lgsdk_Theme_Transparent" parent="AppTheme">
	    <item name="android:windowIsTranslucent">true</item>
	    <item name="android:windowBackground">@drawable/lgsdk_translucence</item>
	    <item name="android:windowContentOverlay">@null</item>
	    <item name="android:windowNoTitle">true</item>
	    <item name="android:backgroundDimEnabled">false</item>
	    <item name="android:windowAnimationStyle">@null</item>
  	</style>
  	
    <style name="lgsdk_TitleTextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/lgsdk_white</item>
        <item name="android:textSize">18sp</item>
    </style>
    
    <style name="lgsdk_ListViewItemTextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/lgsdk_list_title_color</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    
    <style name="lgsdk_NewServerListItemDownloadBtnTextStyle" parent="@style/lgsdk_ListViewItemTextStyle">
        <item name="android:textSize">15sp</item>
    </style>
    
    <style name="lgsdk_NewServerListItemTitleTextStyle" parent="@style/lgsdk_ListViewItemTextStyle">
        <item name="android:textSize">18sp</item>
    </style>
    
    <style name="lgsdk_GameDetailHintTextStyle" parent="@style/lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_account_security_tip_color</item>
        <item name="android:textSize">16sp</item>
    </style>
    
    <style name="lgsdk_GameDetailCongratulateTextStyle" parent="@style/lgsdk_GameDetailHintTextStyle">
        <item name="android:textColor">@color/lgsdk_list_title_color</item>
        <item name="android:textSize">16sp</item>
    </style>
    
    <style name="lgsdk_GameInfoItemTextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/lgsdk_list_title_color</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    
    <style name="lgsdk_GameGiftDetailTextStyle" parent="@style/lgsdk_ListViewItemTextStyle">
        <item name="android:textSize">18sp</item>
    </style>
    
    <style name="lgsdk_ListViewItemHeadTextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/lgsdk_listview_item_head_text_color</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    
    <style name="lgsdk_GalleryItemTextStyle" parent="lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_game_detail_game_name</item>
        <item name="android:textSize">17sp</item>
    </style>
    
    <style name="lgsdk_ListViewItemDescTextStyle" parent="lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_list_title_color</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="lgsdk_GameGiftTextStyle" parent="lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_account_security_tip_color</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="lgsdk_GameInfoTextStyle" parent="lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_list_title_color</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="lgsdk_GameGiftDetailContentTextStyle" parent="lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_list_title_color</item>
        <item name="android:textSize">15sp</item>
    </style>
    
    <style name="lgsdk_GameGiftBottomTextStyle" parent="lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_game_gift_bottom_text_color</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="lgsdk_ListViewItemBottomTextStyle" parent="lgsdk_ListViewItemTextStyle">
        <item name="android:textColor">@color/lgsdk_account_security_tip_color</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="lgsdk_GameDetailNameTextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/lgsdk_game_detail_game_name</item>
        <item name="android:textSize">18sp</item>
        <!-- item name="android:textStyle">bold</item-->
    </style>
    
    <style name="lgsdk_GameDetailCatgoryTextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/lgsdk_game_detail_catgory_name</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="Theme.UPPay" parent="android:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
     <style name="mbssdk_paytype_style" parent="@android:style/Theme.Dialog">  
        <item name="android:windowFrame">@null</item>  
        <item name="android:windowIsFloating">true</item>  
        <item name="android:windowIsTranslucent">true</item>  
        <item name="android:windowNoTitle">true</item>  
        <item name="android:backgroundDimEnabled">false</item>  
        <item name="android:windowBackground">@color/lgsdk_app_background</item>  
    </style>  
</resources>
