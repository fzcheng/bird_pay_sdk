package com.cheyooh.service.sdk.idata;

import com.cheyooh.service.framework.idata.ResultContent;

public class ResultUserLogin extends ResultContent{
 
	private static final long serialVersionUID = -2592466526984032034L;

	
	private String sid;
	
	/**
	 * valid_time表示有效时间，以秒为单位。
	 */
	private int    valid_time;
	
	/**
	 * bindPhone表示是否绑定过手机号 
	 */
	private boolean bind_phone;
	
	
	private String nick_name;
	
	public ResultUserLogin(){
		super("user_login");
	}

	public String getSid() {
		return sid;
	}

	public void setSid(String sid) {
		this.sid = sid;
	}

	public int getValid_time() {
		return valid_time;
	}

	public void setValid_time(int valid_time) {
		this.valid_time = valid_time;
	}

	public boolean isBind_phone() {
		return bind_phone;
	}

	public void setBind_phone(boolean bind_phone) {
		this.bind_phone = bind_phone;
	}

	public String getNick_name() {
		return nick_name;
	}

	public void setNick_name(String nick_name) {
		this.nick_name = nick_name;
	}
	  
	
}
