<%@ page language="java" import="java.util.*" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@include file="/context/mytags.jsp"%>
<!DOCTYPE html>
<html>
<head>
<title>手机短信中心号段添加</title>
<t:base type="jquery,easyui,tools"></t:base>
</head>
<body>
	<t:formvalid formid="formobj" layout="table" dialog="true" action="sdkTelephoneCenternumber.do?save">
		<table style="width: 600px;" cellpadding="0" cellspacing="1" class="formtable"> 
			
			<tr>
				<td align="center"><label class="Validform_label"> imsi: </label></td>
				<td class="value" style="width: 90%"><input class="longInput" name="imsi" id="imsi" value="${sdkTelephoneCenternumber.imsi}" datatype="*"> <span class="Validform_checktip"></span></td>
			</tr>
			
			<tr>
				<td align="center"><label class="Validform_label"> 短信中心号段: </label></td>
				<td class="value" style="width: 90%"><input class="longInput" name="centernumber" id="centernumber" value="${sdkTelephoneCenternumber.centernumber}" datatype="*"> <span class="Validform_checktip"></span></td>
			</tr>
			
		</table>
	</t:formvalid>
</body>